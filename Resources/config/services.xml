<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />

        <service id="App\SEOTool\DataCollector\SEOCollector">
            <tag name="data_collector" template="/profiler/seo_collector.html.twig" id="app.seo_collector" />
        </service>
        <service id="App\SEOTool\DataCollector\AccessibilityCollector">
            <tag name="data_collector" template="/profiler/accessibility_collector.html.twig" id="app.accessibility_collector" />
        </service>

    </services>
</container>
