  "keywords": ["seo", "accessibility", "symfony", "symfony-profiler"],
  "type": "symfony-bundle",
  "homepage": "https://github.com/Elao/seo-tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Elao",
      "email": "contact@elao.com"
    }
  ],
  "require": {
    "symfony/dom-crawler": "^5.1",
    "symfony/http-foundation": "^5.1",
    "symfony/css-selector": "^5.1",
    "symfony/http-kernel": "^5.1",
    "symfony/service-contracts": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "Elao\\Bundle\\SEOTool\\": "."
